    "import os\n",
    "from agents import ConversationManagerAgent, TaskLogicAgent, ToolExecutionAgent\n",
    "from tools import execute_tool"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Initialization ---"",
    "# This setup is the same as in your agents.py\n",
    "# In a notebook, it's good practice to re-initialize to be self-contained.\n",
    "try:\n",
    "    PROJECT_ID = \"adhd-assistant-capstone\"\n",
    "    LOCATION = \"us-central1\"\n",
    "    import vertexai\n",
    "    vertexai.init(project=PROJECT_ID, location=LOCATION)\n",
    "    print(\"Vertex AI Initialized.\")\n",
    "except Exception as e:\n",
    "    print(f\"Vertex AI initialization failed: {e}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Create Agent Instances ---"",
    "task_agent = TaskLogicAgent()\n",
    "tool_agent = ToolExecutionAgent()\n",
    "manager = ConversationManagerAgent(task_agent=task_agent, tool_agent=tool_agent)\n",
    "print(\"Agents created.\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Simulate a User Interaction ---"",
    "user_input = \"I have a big presentation on Friday morning, I need to call the doctor sometime next week to make an appointment, and I also have to remember to buy eggs and bread.\"\n",
    "\n",
    "print(f\"\\n--- Handling User Input ---\\n'{user_input}'\")\n",
    "\n",
    "# Set auto_confirm to False to simulate the user confirmation step (HITL)\n",
    "agent_turn = manager.handle_user_message(\n",
    "    user_text=user_input,\n",
    "    user_id=\"notebook_test_user\",\n",
    "    auto_confirm=False\n",
    ")\n",
    "\n",
    "print(f\"\\n--- Agent's Response to User ---\\n{agent_turn.user_facing_message}\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# --- Simulate the User Confirming the Action ---"",
    "if agent_turn.requires_confirmation:\n",
    "    print(\"\\n--- User Confirms. Executing pending actions... ---\")\n",
    "    results = tool_agent.execute_actions(agent_turn.pending_actions)\n",
    "    \n",
    "    print(\"\\n--- Tool Execution Results ---\")\n",
    "    for res in results:\n",
    "        print(res)\n",
    "else:\n",
    "    print(\"\\n--- No confirmation required or no actions to perform. ---")"
   ]
  }
